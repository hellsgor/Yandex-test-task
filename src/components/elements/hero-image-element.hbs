<picture class="hero__{{#if className}}{{className}}{{else}}{{elemName}}{{/if}}">
  <source srcset="/images/hero/{{elemName}}_1x.webp 1x, /images/hero/{{elemName}}_2x.webp 2x" type="image/webp">
  <source srcset="/images/hero/{{elemName}}_1x.png 1x, /images/hero/{{elemName}}_2x.png 2x" type="image/png">
  <img src="/images/hero/{{elemName}}_1x.png" alt="{{elemName}}">
</picture>
