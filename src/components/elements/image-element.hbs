<picture class="{{parent}}__{{#if className}}{{className}}{{else}}{{elemName}}{{/if}}">
  <source srcset="/images/{{parent}}/{{elemName}}_1x.webp 1x, /images/{{parent}}/{{elemName}}_2x.webp 2x" type="image/webp">
  <source srcset="/images/{{parent}}/{{elemName}}_1x.png 1x, /images/{{parent}}/{{elemName}}_2x.png 2x" type="image/png">
  <img src="/images/{{parent}}/{{elemName}}_1x.png" alt="{{elemName}}">
</picture>
